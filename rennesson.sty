%degré, intervalle : arrivée, départ, fonction
\newcommand{\taylordif}[4]{  \[ #4(#3) = \sum_{k = 0}^{#1} \frac{ \left( #3 - #2 \right)^{k} }{k!} #4^{\left( k \right)} (#2) \] }

%rest_taylorRI_dif : n, a, b, f 
%\int^{b}_{a} \frac{(b-a)}{n!} f^{(n+1)}(t)
%\newcommand{\restTaylorRIdif}[4]{ \int^{b}_{a} \frac{(b - a)}{n!} f^{(n+1)}(t) }
\newcommand{\restTaylorRIdif}[4]{ \int^{#3}_{#2} \frac{(#3 - #2)}{#1 !} #4^{(#1 + 1)}(t) }

%rest_taylorRI_dif : n, a, b, f 
%\frac{(b - a)^{n + 1}}{(n + 1) !} f^{(n + 1)} (c)
%\newcommand{\restTaylorLAdif}[4]{ \frac{(b - a)^{n + 1}}{(n + 1) !} f^{(n + 1)} (c) }
\newcommand{\restTaylorLAdif}[4]{ \frac{(#3 - #2)^{#1 + 1} }{ (#1 +1) !} #4^{(#1 + 1)} (c) }

%rest_taylorY_dpv : n, a, x
%(x - a)^{n} \varepsilon (x) \text{ où } \lim_{x \longrightarrow a} \varepsilon (x) = 0
%\newcommand{\restTaylorYdpv}[4]{ (x - a)^{n} \varepsilon (x) \text{ où } \lim_{x \longrightarrow a} \varepsilon (x) = 0 }
\newcommand{\restTaylorYdpv}[3]{ (#3 - #2)^{#1} \varepsilon (#3) \text{ où } \lim_{#3 \longrightarrow #2} \varepsilon (#3) = 0 }

%tx_acc_if : a, b, f
%\frac{f(b) - f(a)}{b - a}
%\newcommand{\txAccif}[3]{ \frac{ f(b) - f(a) }{b - a} }
\newcommand{\txAccif}[3]{ \frac{ #3 (#2) - #3 (#1) }{ #2 - #1 } }






